    "oda-api-common": "^0.2.0",
    "oda-api-graphql": "^0.3.1",
    "oda-gen-graphql": "^0.3.1",
    "oda-model": "^0.0.11",
    "subscriptions-transport-ws": "^0.8.1"
  },
